<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title VOLLEYTEAM Esbjerg</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*<link rel="stylesheet" type="text/css" href="LayOutCss.css" />*@

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

              

                <a href="@Url.Action("Index", "HomePage")" class = "navbar-brand">
                    <img src="~/Resources/LogoSortBaggrund.jpg" width="60" height="40"/>
                    </a>

               

                
                
                    
                
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Om Klubben", "Index", "About")</li>
                    <li>@Html.ActionLink("Nyheder", "Index", "News")</li>
                    <li>@Html.ActionLink("Holdene", "Index", "Team")</li>
                    <li>@Html.ActionLink("Turneringer", "Index", "Tournament")</li>
                    <li>@Html.ActionLink("Bestyrelsen", "Index", "Board")</li>
                    <li>@Html.ActionLink("Sponsorere", "Index", "Sponsor")</li>
                    <li>@Html.ActionLink("Kontakt", "Index", "Contacts")</li>
                    <li color="#190F4B">@Html.ActionLink("In English","Index","English")</li>


                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
       @RenderBody()
        <hr />
        <footer> <p> volleyteamesbjerg@@mail.com</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
